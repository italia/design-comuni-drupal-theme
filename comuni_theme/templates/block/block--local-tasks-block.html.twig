{% extends "block.html.twig" %}
{#
/**
 * @file
 * Theme override for tabs.
 */
#}
{% block content %}
  {% if content %}
<div class="container pt-2" id="main-container">
<div class="row">
<div class="col-12">
    <nav class="tabs" role="navigation" aria-label="{{ 'Tabs'|t }}">
      {{ content }}
    </nav>
</div>
</div>
</div>
  {% endif %}
{% endblock %}
